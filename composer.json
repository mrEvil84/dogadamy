{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.77",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "php-amqplib/php-amqplib": ">=3.0",
        "tecnickcom/tcpdf": "^6.4",
        "tymon/jwt-auth": "dev-develop"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "overtrue/phplint": "^4.0",
        "phpspec/prophecy": "^1.15",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpcs": [
            "@php ./vendor/bin/phpcs ./app/src/"
        ],
        "phpunit": [
            "phpunit tests -c phpunit.xml"
        ],
        "psalm": [
            "./vendor/bin/psalm --config=psalm.xml --show-info=true"
        ],
        "phplint": [
            "./vendor/bin/phplint ./ --exclude=vendor"
        ],
        "app-start": [
            "docker-compose -f ./docker-compose.yml up -d"
        ],
        "app-stop": [
            "docker-compose -f ./docker-compose.yml stop"
        ],
        "app-elk-start": [
            "docker-compose -f ./elk/docker-compose.yml -f ./elk/extensions/filebeat/filebeat-compose.yml -f ./elk/extensions/metricbeat/metricbeat-compose.yml up -d"
        ],
        "app-elk-stop": [
            "docker-compose -f ./elk/docker-compose.yml -f ./elk/extensions/filebeat/filebeat-compose.yml -f ./elk/extensions/metricbeat/metricbeat-compose.yml stop"
        ]
    },
    "scripts-descriptions": {
      "app-start": "App start containers",
      "app-stop": "App stop containers",
      "app-elk-start": "Elastic stact start containers",
      "app-elk-stop": "Elastic stack stop containers"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
